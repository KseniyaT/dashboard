    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "jasmine-core": "~4.6.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "karma": "~6.4.0",
